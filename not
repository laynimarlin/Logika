# membuat judul tabel
print("tabel kebenaran negasi")
print("_________________________")

# menampilkan header tabel
print("P\t|Q\t|P NOT Q")
print("_________________________")

# menggunakan operator logika 'NOT'
for P in [True, False]:
    for Q in [True, False]:
        negasi = not P
        print(f"{P}\t|{Q}\t|{negasi}")
